{
  "manifest_version": 3,
  "name": "Object Manager",
  "version": "1.0.0",
  "description": "Manage and track objects with add/delete functionality",
  "permissions": [
    "storage",
    "nativeMessaging"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
